{
    "version": 2,
    "builds": [{
            "src": "ems/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxlambdaSize": "15mb", "runtime": "python 3.11.1" }

        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "disDir": "staticfiles_build"
            }


        },

        {

            "version": 2,
            "builds": [{
                "src": "package.json",
                "use": "vercel-sapper"
            }]
        }
    ],

    "routes": [{
            "src": "/staic/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "ems/wsgi.py"
        }
    ]
}